{
  "name": "AMW_angular",
  "version": "0.0.1",
  "description": "Angular 2 based front end for AMW",
  "license": "MIT",
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:docker": "yarn run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "webpack --config config/webpack.prod.js --profile --bail",
    "build": "yarn run build:prod",
    "ci": "yarn run lint && yarn test && yarn run e2e",
    "clean:dist": "yarn run rimraf dist",
    "clean:install": "yarn set progress=false && yarn install",
    "clean:start": "yarn start",
    "clean": "yarn cache clean && yarn run rimraf node_modules doc coverage dist",
    "docker": "docker",
    "docs": "yarn run typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "yarn run e2e --elementExplorer",
    "e2e": "yarn run protractor",
    "github-deploy:dev": "webpack --config config/webpack.github-deploy.js --progress --profile --github-dev",
    "github-deploy:prod": "webpack --config config/webpack.github-deploy.js --progress --profile --github-prod",
    "github-deploy": "yarn run github-deploy:dev",
    "lint": "yarn run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "yarn run clean:dist",
    "prebuild:prod": "yarn run clean:dist",
    "preclean:install": "yarn run clean",
    "preclean:start": "yarn run clean",
    "pree2e": "yarn run webdriver:update --standalone",
    "preversion": "yarn test",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev:hmr": "yarn run server:dev --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "server": "yarn run server:dev",
    "start": "yarn run server:dev",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "yarn run build",
    "watch:dev": "yarn run build:dev --watch",
    "watch:prod": "yarn run build:prod --watch",
    "watch:test": "yarn run test --auto-watch --no-single-run",
    "watch": "yarn run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "yarn run webdriver-manager start",
    "webdriver:update": "yarn run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/platform-server": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@ng-select/ng-select": "0.19.0",
    "angular2-datatable": "0.6.0",
    "bootstrap-3-typeahead": "4.0.2",
    "bootstrap-sass": "3.4.1",
    "bootstrap-tagsinput": "0.7.1",
    "core-js": "2.5.2",
    "eonasdan-bootstrap-datetimepicker": "4.17.47",
    "ie-shim": "0.1.0",
    "jquery": "3.4.1",
    "lodash": "4.17.11",
    "moment": "2.21.0",
    "resolve-url-loader": "2.3.0",
    "rxjs": "5.5.11",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@types/bootstrap": "3.3.36",
    "@types/hammerjs": "2.0.33",
    "@types/jasmine": "2.5.44",
    "@types/jquery": "3.3.29",
    "@types/lodash": "4.14.121",
    "@types/node": "6.0.62",
    "@types/selenium-webdriver": "2.53.39",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "2.6.28",
    "@types/webpack": "3.8.5",
    "angular2-template-loader": "0.6.2",
    "assets-webpack-plugin": "3.8.3",
    "awesome-typescript-loader": "3.4.0",
    "codelyzer": "4.0.2",
    "compression-webpack-plugin": "1.1.11",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "0.28.11",
    "exports-loader": "0.7.0",
    "expose-loader": "0.7.5",
    "file-loader": "1.1.11",
    "gh-pages": "1.1.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.10.0",
    "imports-loader": "0.7.1",
    "install": "0.10.2",
    "istanbul-instrumenter-loader": "2.0.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.1",
    "karma-mocha-reporter": "2.2.4",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.13",
    "node-sass": "4.7.2",
    "parse5": "3.0.1",
    "protractor": "4.0.14",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "script-ext-html-webpack-plugin": "2.0.1",
    "source-map-loader": "0.2.3",
    "string-replace-loader": "2.1.1",
    "style-loader": "0.21.0",
    "to-string-loader": "1.1.5",
    "ts-helpers": "1.1.2",
    "ts-node": "3.0.6",
    "tslint": "5.4.3",
    "tslint-loader": "3.6.0",
    "typedoc": "0.7.2",
    "typescript": "2.3.4",
    "url-loader": "1.0.1",
    "webpack": "3.12.0",
    "webpack-dev-middleware": "2.0.0",
    "webpack-dev-server": "2.11.3",
    "webpack-md5-hash": "0.0.6",
    "webpack-merge": "4.1.3",
    "yarn": "1.3.2"
  },
  "engines": {
    "node": ">= 8.9.0",
    "yarn": ">= 1.3.2"
  }
}
